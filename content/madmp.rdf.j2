    <owl:NamedIndividual rdf:about="http://www.sysresearch.org/rda-common-dmp#afs.server">
        <rdf:type rdf:resource="http://www.sysresearch.org/rda-common-dmp#Host"/>
        <hostID rdf:resource="http://www.sysresearch.org/rda-common-dmp#afs.server.id"/>
        <backupFrequency rdf:datatype="http://www.w3.org/2001/XMLSchema#string">{# maDMP.host.backupFrequency (? = path from madmp JSON schema) #}</backupFrequency>
        <backupType rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></backupType>
        <certifiedWith rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></certifiedWith>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></description>
        <geoLocation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></geoLocation>
        <pidSystem rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></pidSystem>
        <supportsVersioning rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></supportsVersioning>
        <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></title>
    </owl:NamedIndividual>

    {# ... #}
